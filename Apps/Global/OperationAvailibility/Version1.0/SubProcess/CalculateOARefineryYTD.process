                        </xsl:choose>
                    </UnitName>
                    <xsl:choose>
                        <xsl:when test="$FInalizeOARefinery/root/MonthlyOARefinery !=&quot;NaN&quot;">
                            <OAVALUE>
                                <xsl:value-of select="$FInalizeOARefinery/root/MonthlyOARefinery"/>
                            </OAVALUE>
                        </xsl:when>
