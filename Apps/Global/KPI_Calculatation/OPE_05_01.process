<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://xmlns.example.com/unique/default/namespace/1123495989986" xmlns:pfx2="bsr/excelfile/excelsheet/1.0" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx3="bsr/excelfile/appexcel/1.0" xmlns:pfx4="bsr/common/apps/appdataenv/1.0" xmlns:pfx5="bsr/apps/excelfile/appexcel/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="bsr/common/logging/enginelog/1.0" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="bsr/apps/excelfile/excelsheet/1.0" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns5="sbv/common/exceptionhandling/exception/1.0" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
    <xsd:import namespace="http://xmlns.example.com/unique/default/namespace/1123495989986" schemaLocation="/SharedResource/Schema/Global/Excel/ExcelSheet.xsd"/>
    <xsd:import namespace="bsr/common/apps/appdataenv/1.0" schemaLocation="/SharedResource/Schema/Common/AppDataEnvelope.xsd"/>
    <xsd:import namespace="bsr/excelfile/appexcel/1.0"/>
    <xsd:import namespace="bsr/excelfile/excelsheet/1.0"/>
    <xsd:import namespace="bsr/apps/excelfile/appexcel/1.0" schemaLocation="/SharedResource/Schema/Apps/ExcelFile/ExcelSheet.xsd"/>
    <xsd:import namespace="bsr/apps/excelfile/excelsheet/1.0" schemaLocation="/SharedResource/Schema/Apps/ExcelFile/InputExcel.xsd"/>
    <pd:name>Apps/Global/KPI_Calculatation/OPE_05_01.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Actual" type="xsd:double"/>
                    <xsd:element name="CateType" type="xsd:double"/>
                    <xsd:element name="CateID" type="xsd:double"/>
                    <xsd:element name="Date" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>76</pd:startX>
    <pd:startY>204</pd:startY>
    <pd:returnBindings>
        <KPI-Result>
            <Ytd>
                <xsl:value-of select="$KPI-Results/KPIs/YtdKPI"/>
            </Ytd>
            <Mtd>
                <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
            </Mtd>
            <Daily>
                <xsl:value-of select="$KPI-Results/KPIs/DailyKPI"/>
            </Daily>
        </KPI-Result>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="KPI-Result">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Ytd" type="xsd:double"/>
                    <xsd:element name="Mtd" type="xsd:double"/>
                    <xsd:element name="Daily" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>724</pd:endX>
    <pd:endY>204</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <KPI-Results>
            <xsd:element name="KPIs">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="DailyKPI" type="xsd:double" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </KPI-Results>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1400810810798</pd:targetNamespace>
    <pd:activity name="Calculate KPI Result">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:description>Calculate KPI Value: Daily / MTD / Monthly / YTD</pd:description>
        <pd:x>396</pd:x>
        <pd:y>211</pd:y>
        <config>
            <variableName>KPI-Results</variableName>
        </config>
        <pd:inputBindings>
            <KPIs>
                <DailyKPI>
                    <xsl:value-of select="100 * $Start/root/Actual div $queryPlannedPrice/resultSet/Record[1]/PLAN_VALUE"/>
                </DailyKPI>
            </KPIs>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="insertResultTable">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>558</pd:x>
        <pd:y>203</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into OPE_KPI_01_02_RESULT (UNIT_ID, KPI_DAILY_UNIT, KPI_MONTHLY_UNIT, KPI_MTD_UNIT, KPI_YTD_UNIT, DATE_UPDATE)
 values (?, ?, ?, ?, ?, to_date(?,'MM/DD/YYYY'))</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>UNIT_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_DAILY_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_MONTHLY_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_MTD_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_YTD_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>DATE_UPDATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <UNIT_ID>
                    <xsl:value-of select="$Start/root/UnitID"/>
                </UNIT_ID>
                <KPI_DAILY_UNIT>
                    <xsl:value-of select="$KPI-Results/KPIs/DailyKPI"/>
                </KPI_DAILY_UNIT>
                <xsl:choose>
                    <xsl:when test="substring($Start/root/Date, 4, 2) = $GetLastDayOfMonth/LastDay">
                        <KPI_MONTHLY_UNIT>
                            <xsl:choose>
                                <xsl:when test="exists($KPI-Results/KPIs/MtdKPI)">
                                    <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </KPI_MONTHLY_UNIT>
                    </xsl:when>
                    <xsl:otherwise>
                        <KPI_MONTHLY_UNIT xsi:nil="true"/>
                    </xsl:otherwise>
                </xsl:choose>
                <KPI_MTD_UNIT>
                    <xsl:choose>
                        <xsl:when test="exists($KPI-Results/KPIs/MtdKPI)">
                            <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </KPI_MTD_UNIT>
                <KPI_YTD_UNIT>
                    <xsl:value-of select="$KPI-Results/KPIs/YtdKPI"/>
                </KPI_YTD_UNIT>
                <DATE_UPDATE>
                    <xsl:value-of select="$Start/root/Date"/>
                </DATE_UPDATE>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="queryPlannedPrice">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:description/>
        <pd:x>240</pd:x>
        <pd:y>202</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>select PLAN_VALUE from OPE_KPI_05_01_DETAILS 
where VALID_DATE = to_date(?,'MM/DD/YYYY')  and CATE_ID = ? and CATE_TYPE = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>VALID_DATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>CATE_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>CATE_TYPE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>PLAN_VALUE</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <VALID_DATE>
                    <xsl:value-of select="$Start/root/Date"/>
                </VALID_DATE>
                <CATE_ID>
                    <xsl:value-of select="$Start/root/CateID"/>
                </CATE_ID>
                <CATE_TYPE>
                    <xsl:value-of select="$Start/root/CateType"/>
                </CATE_TYPE>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Calculate KPI Result</pd:from>
        <pd:to>insertResultTable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>queryPlannedPrice</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertResultTable</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>queryPlannedPrice</pd:from>
        <pd:to>Calculate KPI Result</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>