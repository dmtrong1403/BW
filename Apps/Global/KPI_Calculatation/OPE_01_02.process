<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://xmlns.example.com/unique/default/namespace/1123495989986" xmlns:pfx2="bsr/excelfile/excelsheet/1.0" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx3="bsr/excelfile/appexcel/1.0" xmlns:pfx4="bsr/common/apps/appdataenv/1.0" xmlns:pfx5="bsr/apps/excelfile/appexcel/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="bsr/common/logging/enginelog/1.0" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="bsr/apps/excelfile/excelsheet/1.0" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns5="sbv/common/exceptionhandling/exception/1.0" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
    <xsd:import namespace="http://xmlns.example.com/unique/default/namespace/1123495989986" schemaLocation="/SharedResource/Schema/Global/Excel/ExcelSheet.xsd"/>
    <xsd:import namespace="bsr/common/apps/appdataenv/1.0" schemaLocation="/SharedResource/Schema/Common/AppDataEnvelope.xsd"/>
    <xsd:import namespace="bsr/excelfile/appexcel/1.0"/>
    <xsd:import namespace="bsr/excelfile/excelsheet/1.0"/>
    <xsd:import namespace="bsr/apps/excelfile/appexcel/1.0" schemaLocation="/SharedResource/Schema/Apps/ExcelFile/ExcelSheet.xsd"/>
    <xsd:import namespace="bsr/apps/excelfile/excelsheet/1.0" schemaLocation="/SharedResource/Schema/Apps/ExcelFile/InputExcel.xsd"/>
    <pd:name>Apps/Global/KPI_Calculatation/OPE_01_02.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Actual" type="xsd:double"/>
                    <xsd:element name="UnitID" type="xsd:double"/>
                    <xsd:element name="Date" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>47</pd:startX>
    <pd:startY>206</pd:startY>
    <pd:returnBindings>
        <KPI-Result>
            <Ytd>
                <xsl:value-of select="$KPI-Results/KPIs/YtdKPI"/>
            </Ytd>
            <Mtd>
                <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
            </Mtd>
            <Daily>
                <xsl:value-of select="$KPI-Results/KPIs/DailyKPI"/>
            </Daily>
        </KPI-Result>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="KPI-Result">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Ytd" type="xsd:double"/>
                    <xsd:element name="Mtd" type="xsd:double"/>
                    <xsd:element name="Daily" type="xsd:double"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>1109</pd:endX>
    <pd:endY>208</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <KPI-Results>
            <xsd:element name="KPIs">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="MtdKPI" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="DailyKPI" type="xsd:double" minOccurs="0"/>
                        <xsd:element name="YtdKPI" type="xsd:double"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </KPI-Results>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1400810810798</pd:targetNamespace>
    <pd:activity name="queryDesignCapacity">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:description>Query Actual &amp; Design Capacity of Process Units from OPE_KPI_01_03_DETAILS</pd:description>
        <pd:x>157</pd:x>
        <pd:y>206</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>select DESIGN_CAPACITY from OPE_KPI_01_03_DATA where DATE_UPDATE = to_date(?,'MM/DD/YYYY')  and UNIT_ID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>DATE_UPDATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>UNIT_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>DESIGN_CAPACITY</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <DATE_UPDATE>
                    <xsl:value-of select="$Start/root/Date"/>
                </DATE_UPDATE>
                <UNIT_ID>
                    <xsl:value-of select="$Start/root/UnitID"/>
                </UNIT_ID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Calculate KPI Result">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:description>Calculate KPI Value: Daily / MTD / Monthly / YTD</pd:description>
        <pd:x>782</pd:x>
        <pd:y>217</pd:y>
        <config>
            <variableName>KPI-Results</variableName>
        </config>
        <pd:inputBindings>
            <KPIs>
                <MtdKPI>
                    <xsl:value-of select="100 * $queryMtdCapacityByUnit/resultSet/Record[1]/SUMACTUAL div $queryMtdCapacityByUnit/resultSet/Record[1]/SUMDESIGN_CAPACITY"/>
                </MtdKPI>
                <DailyKPI>
                    <xsl:value-of select="100 * $Start/root/Actual div $queryDesignCapacity/resultSet/Record[1]/DESIGN_CAPACITY"/>
                </DailyKPI>
                <YtdKPI>
                    <xsl:value-of select="100 * $queryYtdCapacity/resultSet/Record[1]/AVGACTUAL div $queryYtdCapacity/resultSet/Record[1]/AVGDESIGN_CAPACITY"/>
                </YtdKPI>
            </KPIs>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="insertResultTable">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>924</pd:x>
        <pd:y>209</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into OPE_KPI_01_02_RESULT (UNIT_ID, KPI_DAILY_UNIT, KPI_MONTHLY_UNIT, KPI_MTD_UNIT, KPI_YTD_UNIT, DATE_UPDATE)
 values (?, ?, ?, ?, ?, to_date(?,'MM/DD/YYYY'))</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>UNIT_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_DAILY_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_MONTHLY_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_MTD_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>KPI_YTD_UNIT</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
                <parameter>
                    <parameterName>DATE_UPDATE</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <UNIT_ID>
                    <xsl:value-of select="$Start/root/UnitID"/>
                </UNIT_ID>
                <KPI_DAILY_UNIT>
                    <xsl:value-of select="$KPI-Results/KPIs/DailyKPI"/>
                </KPI_DAILY_UNIT>
                <xsl:choose>
                    <xsl:when test="substring($Start/root/Date, 4, 2) = $GetLastDayOfMonth/LastDay">
                        <KPI_MONTHLY_UNIT>
                            <xsl:choose>
                                <xsl:when test="exists($KPI-Results/KPIs/MtdKPI)">
                                    <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
                                </xsl:when>
                                <xsl:otherwise>
                                    <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                </xsl:otherwise>
                            </xsl:choose>
                        </KPI_MONTHLY_UNIT>
                    </xsl:when>
                    <xsl:otherwise>
                        <KPI_MONTHLY_UNIT xsi:nil="true"/>
                    </xsl:otherwise>
                </xsl:choose>
                <KPI_MTD_UNIT>
                    <xsl:choose>
                        <xsl:when test="exists($KPI-Results/KPIs/MtdKPI)">
                            <xsl:value-of select="$KPI-Results/KPIs/MtdKPI"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </KPI_MTD_UNIT>
                <KPI_YTD_UNIT>
                    <xsl:value-of select="$KPI-Results/KPIs/YtdKPI"/>
                </KPI_YTD_UNIT>
                <DATE_UPDATE>
                    <xsl:value-of select="$Start/root/Date"/>
                </DATE_UPDATE>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="queryMtdCapacityByUnit">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>302</pd:x>
        <pd:y>206</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>select SUM(ACTUAL), SUM(DESIGN_CAPACITY)
from OPE_KPI_01_03_DATA 
where DATE_UPDATE &gt;= ? and DATE_UPDATE &lt;= to_date(?,'MM/DD/YYYY') and UNIT_ID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>startDate</parameterName>
                    <dataType>DATE</dataType>
                </parameter>
                <parameter>
                    <parameterName>endDate</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>UNIT_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>SUMACTUAL</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>SUMDESIGN_CAPACITY</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <startDate>
                    <xsl:value-of select="tib:create-date(tib:substring-after-last($Start/root/Date,&quot;/&quot;), substring($Start/root/Date, 1, 2), 01)"/>
                </startDate>
                <endDate>
                    <xsl:value-of select="$Start/root/Date"/>
                </endDate>
                <UNIT_ID>
                    <xsl:value-of select="$Start/root/UnitID"/>
                </UNIT_ID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetLastDayOfMonth">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>620</pd:x>
        <pd:y>208</pd:y>
        <config>
            <processName>/Apps/Global/Common/GetLastDayOfMonth.process</processName>
        </config>
        <pd:inputBindings>
            <InputDate>
                <xsl:value-of select="tib:create-date(tib:substring-after-last($Start/root/Date,&quot;/&quot;), substring($Start/root/Date, 1, 2), 01)"/>
            </InputDate>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="queryYtdCapacity">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>451</pd:x>
        <pd:y>207</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>select AVG(ACTUAL), AVG(DESIGN_CAPACITY)
from OPE_KPI_01_03_DATA 
where DATE_UPDATE &gt;= ? and DATE_UPDATE &lt;= to_date(?,'MM/DD/YYYY') and UNIT_ID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>startDate</parameterName>
                    <dataType>DATE</dataType>
                </parameter>
                <parameter>
                    <parameterName>endDate</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>UNIT_ID</parameterName>
                    <dataType>NUMERIC</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>AVGACTUAL</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>AVGDESIGN_CAPACITY</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>2</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>NUMBER</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <startDate>
                    <xsl:value-of select="tib:create-date(tib:substring-after-last($Start/root/Date,&quot;/&quot;), 01, 01)"/>
                </startDate>
                <endDate>
                    <xsl:value-of select="$Start/root/Date"/>
                </endDate>
                <UNIT_ID>
                    <xsl:value-of select="$Start/root/UnitID"/>
                </UNIT_ID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Calculate KPI Result</pd:from>
        <pd:to>insertResultTable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>queryMtdCapacityByUnit</pd:from>
        <pd:to>queryYtdCapacity</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>queryDesignCapacity</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>queryDesignCapacity</pd:from>
        <pd:to>queryMtdCapacityByUnit</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertResultTable</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>queryYtdCapacity</pd:from>
        <pd:to>GetLastDayOfMonth</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetLastDayOfMonth</pd:from>
        <pd:to>Calculate KPI Result</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>