        </config>
        <pd:inputBindings>
            <ns5:Exception>
                <pfx4:Metadata>
                    <pfx4:AppCode>
                        <xsl:value-of select="&quot;ProcessDailyExcel&quot;"/>
                    </pfx4:AppCode>
                    <pfx4:Version>
                        <xsl:value-of select="&quot;1.0&quot;"/>
                    </pfx4:Version>
                    <pfx4:ID>
                        <xsl:value-of select="concat(generate-id($_processContext), tib:timestamp())"/>
