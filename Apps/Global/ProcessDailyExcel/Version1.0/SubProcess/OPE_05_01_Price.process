            <pd:y>211</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>false</emptyStrAsNil>
                <statement>update OPE_KPI_05_01_DETAILS set ACTUAL_VALUE = ? where CATE_ID = ? and CATE_TYPE = 0  and valid_date = to_date(?,'MM/DD/YYYY')</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>ACTUAL_VALUE</parameterName>
                        <dataType>NUMERIC</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>CATE_ID</parameterName>
                        <dataType>NUMERIC</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>VALID_DATE</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
                <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:choose>
                        <xsl:when test="string(number($xrecords/pfx5:row/pfx5:cell[number($columnOffset/Offsets/Actual_Offset)]/pfx5:value)) !='NaN'">
                            <ACTUAL_VALUE>
                                <xsl:choose>
                                    <xsl:when test="exists($xrecords/pfx5:row/pfx5:cell[number($columnOffset/Offsets/Actual_Offset)]/pfx5:value)">
                                        <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[number($columnOffset/Offsets/Actual_Offset)]/pfx5:value"/>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:attribute name="xsi:nil">true</xsl:attribute>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </ACTUAL_VALUE>
                        </xsl:when>
                        <xsl:otherwise>
                            <ACTUAL_VALUE xsi:nil="true"/>
                        </xsl:otherwise>
                    </xsl:choose>
                    <CATE_ID>
                        <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[number($columnOffset/Offsets/ID_Offset)]/pfx5:value"/>
                    </CATE_ID>
                    <VALID_DATE>
                        <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[number($columnOffset/Offsets/Date_Offset)]/pfx5:value"/>
