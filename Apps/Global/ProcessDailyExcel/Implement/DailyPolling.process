<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="bsr/common/apps/appdataenv/1.0" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="bsr/excelfile/excelsheet/1.0" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx3="sbv/common/exceptionhandling/exception/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="bsr/apps/excelfile/excelsheet/1.0">
    <xsd:import namespace="bsr/common/apps/appdataenv/1.0" schemaLocation="/SharedResource/Schema/Common/AppDataEnvelope.xsd"/>
    <xsd:import namespace="bsr/excelfile/excelsheet/1.0"/>
    <xsd:import namespace="bsr/apps/excelfile/excelsheet/1.0" schemaLocation="/SharedResource/Schema/Apps/ExcelFile/InputExcel.xsd"/>
    <pd:name>Apps/Global/ProcessDailyExcel/Implement/DailyPolling.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>87</pd:x>
        <pd:y>65</pd:y>
        <config>
            <pollInterval>%%APPsInfo/Polling/OPDaily/Interval%%</pollInterval>
            <createEvent>true</createEvent>
            <modifyEvent>false</modifyEvent>
            <deleteEvent>false</deleteEvent>
            <mode>files-and-directories</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <fileName>%%APPsInfo/Polling/OPDaily/FileName%%</fileName>
            <includeCurrent>true</includeCurrent>
            <excludeContent>true</excludeContent>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>733</pd:endX>
    <pd:endY>69</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1457944554017</pd:targetNamespace>
    <pd:activity name="MainProcessOperation">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>261</pd:x>
        <pd:y>65</pd:y>
        <config>
            <processName>/Apps/TemplateProcess/MainProcessOperation.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <pfx:AppDataEnvelope>
                    <pfx:Metadata>
                        <pfx:AppCode>
                            <xsl:value-of select="&quot;ProcessDailyExcel&quot;"/>
                        </pfx:AppCode>
                        <pfx:Version>
                            <xsl:value-of select="&quot;1.0&quot;"/>
                        </pfx:Version>
                        <pfx:ID>
                            <xsl:value-of select="concat(generate-id($_processContext/ns1:ProcessContext), tib:timestamp())"/>
                        </pfx:ID>
                        <pfx:CreateTime>
                            <xsl:value-of select="current-dateTime()"/>
                        </pfx:CreateTime>
                    </pfx:Metadata>
                    <pfx:AppData>
                        <ns3:InputExcel>
                            <ns3:FileName>
                                <xsl:value-of select="$File-Poller/ns2:EventSourceOuputNoContentClass/fileInfo/fullName"/>
                            </ns3:FileName>
                            <ns3:SheetName>
                                <xsl:value-of select="$_globalVariables/ns:GlobalVariables/APPsInfo/Polling/OPDaily/SheetName"/>
                            </ns3:SheetName>
                            <ns3:rowCount>
                                <xsl:value-of select="&quot;&quot;"/>
                            </ns3:rowCount>
                            <ns3:colCount>
                                <xsl:value-of select="&quot;&quot;"/>
                            </ns3:colCount>
                            <ns3:rowOffset>
                                <xsl:value-of select="&quot;&quot;"/>
                            </ns3:rowOffset>
                            <ns3:colOffset>
                                <xsl:value-of select="&quot;&quot;"/>
                            </ns3:colOffset>
                        </ns3:InputExcel>
                    </pfx:AppData>
                </pfx:AppDataEnvelope>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>400</pd:x>
        <pd:y>193</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="handleException">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>590</pd:x>
        <pd:y>194</pd:y>
        <config>
            <processName>/Util/Exception/handleException.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:Exception>
                <pfx:Metadata>
                    <pfx:AppCode>
                        <xsl:value-of select="&quot;ProcessDailylyExcel&quot;"/>
                    </pfx:AppCode>
                    <pfx:Version>
                        <xsl:value-of select="&quot;1.0&quot;"/>
                    </pfx:Version>
                    <pfx:ID>
                        <xsl:value-of select="concat(generate-id($_processContext/ns1:ProcessContext), tib:timestamp())"/>
                    </pfx:ID>
                    <pfx:CreateTime>
                        <xsl:value-of select="current-dateTime()"/>
                    </pfx:CreateTime>
                </pfx:Metadata>
                <pfx3:ExceptionBody>
                    <pfx3:ErrorReport>
                        <StackTrace>
                            <xsl:value-of select="$_error/ns1:ErrorReport/StackTrace"/>
                        </StackTrace>
                        <Msg>
                            <xsl:value-of select="$_error/ns1:ErrorReport/Msg"/>
                        </Msg>
                        <FullClass>
                            <xsl:value-of select="$_error/ns1:ErrorReport/FullClass"/>
                        </FullClass>
                        <ObjectClass>
                            <xsl:value-of select="$_error/ns1:ErrorReport/Class"/>
                        </ObjectClass>
                        <ProcessStack>
                            <xsl:value-of select="$_error/ns1:ErrorReport/ProcessStack"/>
                        </ProcessStack>
                        <xsl:if test="$_error/ns1:ErrorReport/MsgCode">
                            <MsgCode>
                                <xsl:value-of select="$_error/ns1:ErrorReport/MsgCode"/>
                            </MsgCode>
                        </xsl:if>
                        <xsl:copy-of select="$_error/ns1:ErrorReport/Data"/>
                    </pfx3:ErrorReport>
                </pfx3:ExceptionBody>
            </pfx3:Exception>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Backup Daily File To Server">
        <pd:type>com.tibco.plugin.file.FileCopyActivity</pd:type>
        <pd:resourceType>ae.activities.FileCopyActivity</pd:resourceType>
        <pd:x>408</pd:x>
        <pd:y>73</pd:y>
        <config>
            <overwrite>true</overwrite>
        </config>
        <pd:inputBindings>
            <ns2:CopyActivityConfig>
                <fromFileName>
                    <xsl:value-of select="$File-Poller/ns2:EventSourceOuputNoContentClass/fileInfo/fullName"/>
                </fromFileName>
                <toFileName>
                    <xsl:value-of select="concat($_globalVariables/ns:GlobalVariables/APPsInfo/Polling/OPDaily/Backup_Daily, $File-Poller/ns2:EventSourceOuputNoContentClass/fileInfo/fileName)"/>
                </toFileName>
            </ns2:CopyActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Remove Origin File">
        <pd:type>com.tibco.plugin.file.FileRemoveActivity</pd:type>
        <pd:resourceType>ae.activities.FileRemoveActivity</pd:resourceType>
        <pd:x>563</pd:x>
        <pd:y>68</pd:y>
        <config/>
        <pd:inputBindings>
            <ns2:RemoveActivityInputClass>
                <fileName>
                    <xsl:value-of select="$File-Poller/ns2:EventSourceOuputNoContentClass/fileInfo/fullName"/>
                </fileName>
            </ns2:RemoveActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>MainProcessOperation</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>MainProcessOperation</pd:from>
        <pd:to>Backup Daily File To Server</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>handleException</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>handleException</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Backup Daily File To Server</pd:from>
        <pd:to>Remove Origin File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove Origin File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>