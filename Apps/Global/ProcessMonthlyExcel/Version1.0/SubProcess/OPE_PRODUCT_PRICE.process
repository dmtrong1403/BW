    <pd:group name="DaysofMonth">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>546</pd:x>
        <pd:y>165</pd:y>
        <pd:width>423</pd:width>
        <pd:height>234</pd:height>
        <pd:collapsedWidth>52</pd:collapsedWidth>
        <pd:collapsedHeight>69</pd:collapsedHeight>
        <pd:groupVariables/>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$i &gt;= $GetLastDayOfMonth/LastDay</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>ForEachRow</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ForEachRow</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:group name="ForEachRow">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>598</pd:x>
            <pd:y>206</pd:y>
            <pd:width>317</pd:width>
            <pd:height>159</pd:height>
            <pd:collapsedWidth>52</pd:collapsedWidth>
            <pd:collapsedHeight>69</pd:collapsedHeight>
            <pd:groupVariables/>
            <config>
                <pd:groupType>inputLoop</pd:groupType>
                <pd:serializable>false</pd:serializable>
                <pd:over>$ReadExcelCommon/pfx5:sheet/pfx5:row</pd:over>
                <pd:iterationElementSlot>xrecords</pd:iterationElementSlot>
                <pd:indexSlot>n</pd:indexSlot>
                <pd:activityOutputName/>
                <pd:accumulateOutput>false</pd:accumulateOutput>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>OPE_05_01</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>OPE_05_02</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>OPE_05_01</pd:from>
                <pd:to>OPE_05_02</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="OPE_05_01">
                <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
                <pd:resourceType>ae.process.subprocess</pd:resourceType>
                <pd:x>676</pd:x>
                <pd:y>294</pd:y>
                <config>
                    <processName>/Apps/Global/ProcessMonthlyExcel/Version1.0/SubProcess/OPE_05_01_PRICE.process</processName>
                </config>
                <pd:inputBindings>
                    <root>
                        <Plan>
                            <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[3]/pfx5:value"/>
                        </Plan>
                        <CateID>
                            <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[1]/pfx5:value"/>
                        </CateID>
                        <validDate>
                            <xsl:value-of select="$ConvertData/ValidDateMonth"/>
                        </validDate>
                        <inputDate>
                            <xsl:value-of select="$Map-Data/inputDate"/>
                        </inputDate>
                        <i>
                            <xsl:value-of select="$i"/>
                        </i>
                    </root>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="OPE_05_02">
                <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
                <pd:resourceType>ae.process.subprocess</pd:resourceType>
                <pd:x>842</pd:x>
                <pd:y>294</pd:y>
                <config>
                    <processName>/Apps/Global/ProcessMonthlyExcel/Version1.0/SubProcess/OPE_05_02_PRICE.process</processName>
                </config>
                <pd:inputBindings>
                    <root>
                        <Plan>
                            <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[3]/pfx5:value"/>
                        </Plan>
                        <ProductID>
                            <xsl:value-of select="$xrecords/pfx5:row/pfx5:cell[1]/pfx5:value"/>
                        </ProductID>
                        <validDate>
                            <xsl:value-of select="$ConvertData/ValidDateMonth"/>
                        </validDate>
                        <inputDate>
                            <xsl:value-of select="$Map-Data/inputDate"/>
                        </inputDate>
                        <i>
                            <xsl:value-of select="$i"/>
                        </i>
                    </root>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
