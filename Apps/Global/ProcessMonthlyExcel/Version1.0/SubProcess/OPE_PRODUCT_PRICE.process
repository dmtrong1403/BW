                    </ns1:rowOffset>
                    <ns1:colOffset>
                        <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/APPsInfo/Polling/OPMonthly/KPI_PRODUCT_PRICE/colOffset"/>
                    </ns1:colOffset>
                </ns1:AppExcel>
            </root>
        </pd:inputBindings>
    </pd:activity>
