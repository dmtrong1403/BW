    <pd:activity name="GetLastDayOfMonth">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>454</pd:x>
        <pd:y>173</pd:y>
        <config>
            <processName>/Apps/Global/Common/GetLastDayOfMonth.process</processName>
        </config>
        <pd:inputBindings>
            <InputDate>
                <xsl:value-of select="$Map-Data/inputDate"/>
            </InputDate>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>305</pd:x>
        <pd:y>170</pd:y>
        <config>
            <element>
                <xsd:element name="inputDate" type="xsd:date"/>
            </element>
        </config>
        <pd:inputBindings>
            <inputDate>
                <xsl:value-of select="tib:create-date(tib:substring-after-last($ReadExcelCommon/pfx5:sheet/pfx5:row[1]/pfx5:cell[4]/pfx5:value,&quot;/&quot;), tib:substring-before-last($ReadExcelCommon/pfx5:sheet/pfx5:row[1]/pfx5:cell[4]/pfx5:value, &quot;/&quot;), 01)"/>
            </inputDate>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="query_Valid_Date">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>616</pd:x>
        <pd:y>173</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
            <statement>select TO_CHAR(VALID_DATE,'MM/dd/YYYY') as VALID_DATE from OPE_KPI_03_01_RESULT</statement>
            <wizardData/>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>VALID_DATE</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR2</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>OptionalElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput/>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>716</pd:x>
        <pd:y>13</pd:y>
        <pd:width>385</pd:width>
        <pd:height>319</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$i &gt;= $GetLastDayOfMonth/LastDay</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>compareDate</pd:to>
            <pd:xpathDescription>exists</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>tib:format-date('MM/dd/yyyy', tib:add-to-date($Map-Data/inputDate, 0, 0, ($i - 1))) = $query_Valid_Date/resultSet/Record/VALID_DATE</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>insert2DB</pd:to>
            <pd:xpathDescription>otherwise</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>updateDB</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>insert2DB</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>compareDate</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>compareDate</pd:from>
            <pd:to>updateDB</pd:to>
            <pd:xpathDescription>compare</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>tib:compare-date( tib:add-to-date($Map-Data/inputDate, 0, 0, ($i - 1)) ,$ConvertData/ValidDateMonth ) &gt;= 0</pd:xpath>
        </pd:transition>
        <pd:activity name="insert2DB">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>913</pd:x>
            <pd:y>78</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>true</emptyStrAsNil>
                <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
                <statement>insert into OPE_KPI_03_01_RESULT( PLANNED_TOTAL_CONSUMPTION, VALID_DATE)
values (TO_NUMBER(?),TO_DATE(?,'MM/dd/YYYY'))</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>mPlan</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>VALID_DATE</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:choose>
                        <xsl:when test="string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[7]/pfx5:cell[2]/pfx5:value)) = 'NaN'&#xA;and&#xA;string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[8]/pfx5:cell[2]/pfx5:value)) = 'NaN'">
                            <mPlan xsi:nil="true"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <mPlan>
                                <xsl:value-of select="24 * ($ReadExcelCommon/pfx5:sheet/pfx5:row[7]/pfx5:cell[2]/pfx5:value + $ReadExcelCommon/pfx5:sheet/pfx5:row[8]/pfx5:cell[2]/pfx5:value)"/>
                            </mPlan>
                        </xsl:otherwise>
                    </xsl:choose>
                    <VALID_DATE>
                        <xsl:value-of select="tib:format-date('MM/dd/yyyy', tib:add-to-date($Map-Data/inputDate, 0, 0, ($i - 1)))"/>
                    </VALID_DATE>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="updateDB">
            <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
            <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
            <pd:x>912</pd:x>
            <pd:y>273</pd:y>
            <config>
                <timeout>10</timeout>
                <commit>false</commit>
                <emptyStrAsNil>true</emptyStrAsNil>
                <jdbcSharedConfig>/SharedResource/Connection/JDBC/KPIsDBConnection.sharedjdbc</jdbcSharedConfig>
                <statement>UPDATE OPE_KPI_03_01_RESULT
SET PLANNED_TOTAL_CONSUMPTION = TO_NUMBER(?)
WHERE  VALID_DATE = TO_DATE(?,'MM/dd/YYYY')</statement>
                <Prepared_Param_DataType>
                    <parameter>
                        <parameterName>MonthlyValue</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>DateUpdate</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <xsl:choose>
                        <xsl:when test="string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[7]/pfx5:cell[2]/pfx5:value)) = 'NaN'&#xA;and&#xA;string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[8]/pfx5:cell[2]/pfx5:value)) = 'NaN'">
                            <MonthlyValue xsi:nil="true"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <MonthlyValue>
                                <xsl:value-of select="24 * (&#xA;(if(string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[7]/pfx5:cell[2]/pfx5:value )) = 'NaN') then 0 else $ReadExcelCommon/pfx5:sheet/pfx5:row[7]/pfx5:cell[2]/pfx5:value)&#xA;+ &#xA;(if(string(number($ReadExcelCommon/pfx5:sheet/pfx5:row[8]/pfx5:cell[2]/pfx5:value )) = 'NaN') then 0 else $ReadExcelCommon/pfx5:sheet/pfx5:row[8]/pfx5:cell[2]/pfx5:value)&#xA;)"/>
                            </MonthlyValue>
                        </xsl:otherwise>
                    </xsl:choose>
                    <DateUpdate>
                        <xsl:value-of select="tib:format-date('MM/dd/yyyy', tib:add-to-date($Map-Data/inputDate, 0, 0, ($i - 1)))"/>
                    </DateUpdate>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="compareDate">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>909</pd:x>
            <pd:y>178</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>1058</pd:x>
        <pd:y>384</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
