            <pd:x>675</pd:x>
            <pd:y>35</pd:y>
            <pd:width>500</pd:width>
            <pd:height>454</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
