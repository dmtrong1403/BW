            <xsl:value-of select="if(string(number($CheckMonthlyViolation/IsViolatated)) !='NaN') then number($CheckMonthlyViolation/IsViolatated) + number($CheckDailyViolation/IsViolatated) + number($CheckMtdViolation/IsViolatated) + number($CheckYtdViolation/IsViolatated) else number($CheckDailyViolation/IsViolatated) + number($CheckMtdViolation/IsViolatated) + number($CheckYtdViolation/IsViolatated)"/>
        </IsViolated>
