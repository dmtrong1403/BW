                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Monthly" type="xsd:string"/>
                        <xsd:element name="Ytd" type="xsd:string"/>
                        <xsd:element name="Mtd" type="xsd:string"/>
                        <xsd:element name="Daily" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
